   "source": [
    "f = open('input')\n",
    "data = f.read()\n",
    "offset = int(len(data)/2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "count = 0\n",
    "for i, c in enumerate(data):\n",
    "    if i == offset:\n",
    "        break\n",
    "    elif c == data[i + offset]:\n",
    "        count += int(c)\n",
    "count = count * 2"
   ]
  },
